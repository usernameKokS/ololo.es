  "repository": {
    "type": "git",
    "url": "https://github.com/PygmySlowLoris/vue-stepper"
  },
  "keywords": [
    "vue",
    "stepper",
    "vue-stepper",
    "steps"
  ],
  "author": {
    "name": "PygmySlowLoris Team",
    "email": "team@pygmyslowloris.org",
    "url": "https://github.com/PygmySlowLoris"
  },
  "contributors": [
    "Guido Ceraso <guidoceraso@gmail.com> (https://github.com/hazzo)",
